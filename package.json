{
  "name": "@modernice/vue-i18n-modules",
  "version": "0.0.12",
  "description": "\"Message modules\" extension for vue-i18n",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.mjs",
      "require": "./dist/vite/index.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt/index.d.ts",
      "import": "./dist/nuxt/index.mjs",
      "require": "./dist/nuxt/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "play": "pnpm nuxi dev playground",
    "build": "unbuild",
    "watch": "BUILD_WATCH=1 chokidar \"./src/**/*.ts\" -c \"pnpm build\" --initial",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepack": "pnpm build"
  },
  "keywords": [
    "vue",
    "i18n",
    "modules",
    "vue-i18n",
    "vue-i18n-modules",
    "namespaces",
    "nuxt"
  ],
  "files": [
    "dist",
    "index.d.ts",
    "nuxt.d.ts",
    "vite.d.ts",
    "package.json"
  ],
  "author": "Saman Hosseini <saman@modernice.ltd>",
  "license": "proprietary",
  "dependencies": {
    "@nuxt/kit": "^3.4.3",
    "@nuxt/schema": "^3.4.3"
  },
  "devDependencies": {
    "@intlify/core-base": "^9.2.2",
    "@types/node": "^18.16.7",
    "@vue/reactivity": "^3.2.47",
    "@vue/runtime-core": "^3.2.47",
    "@vue/runtime-dom": "^3.2.47",
    "@vue/shared": "^3.2.47",
    "chokidar-cli": "^3.0.0",
    "nuxt": "^3.4.3",
    "typescript": "^5.0.4",
    "unbuild": "^0.8.8",
    "vue": "3.2.47",
    "vue-i18n": "^9.3.0-beta.16"
  },
  "peerDependencies": {
    "@intlify/core-base": "^9.2.2",
    "@vue/reactivity": "^3.2.47",
    "@vue/runtime-core": "^3.2.47",
    "@vue/runtime-dom": "^3.2.47",
    "@vue/shared": "^3.2.47",
    "vue": "^3.2.47",
    "vue-i18n": "^9.3.0-beta.16"
  }
}
