{
  "name": "@modernice/vue-i18n-modules",
  "version": "0.2.0",
  "description": "\"Message modules\" extension for vue-i18n",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.mjs",
      "require": "./dist/vite/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "vite build",
    "prepack": "vite build",
    "watch": "BUILD_WATCH=1 chokidar \"./src/**/*.ts\" -c \"pnpm build\" --initial"
  },
  "keywords": [
    "vue",
    "i18n",
    "modules",
    "vue-i18n",
    "vue-i18n-modules",
    "namespaces",
    "nuxt"
  ],
  "files": [
    "dist",
    "index.d.ts",
    "nuxt.d.ts",
    "vite.d.ts",
    "package.json"
  ],
  "author": "Saman Hosseini <saman@modernice.ltd>",
  "license": "MIT",
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.2",
    "@intlify/core-base": "9.14.1",
    "@types/node": "^22.7.2",
    "@vue/reactivity": "^3.5.9",
    "@vue/runtime-core": "^3.5.9",
    "@vue/runtime-dom": "^3.5.9",
    "@vue/shared": "^3.5.9",
    "chokidar-cli": "^3.0.0",
    "eslint-config-prettier": "^9.1.0",
    "knitwork": "^1.1.0",
    "pkg-types": "^1.2.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.2",
    "vue": "^3.5.9",
    "vue-i18n": "^9.14.1"
  },
  "peerDependencies": {
    "@vue/reactivity": "^3.3.4",
    "@vue/runtime-core": "^3.3.4",
    "@vue/runtime-dom": "^3.3.4",
    "@vue/shared": "^3.3.4",
    "vue": "^3.3.4",
    "vue-i18n": "^9.4.1"
  }
}
